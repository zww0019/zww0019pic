        ext1: ext1,
        beforeRequest: function(msg) {
          if(msg.logs && msg.logType === 'log') {
            var logLevel = msg.logs.level || '';
            var logMsg = msg.logs.msg || '';
            if(logLevel === '16' || logLevel === '1024') {
              // 接口异常过滤
              if(
                logMsg.indexOf('x/api/wuji_cache/object') !== -1 || 
                logMsg.indexOf('dp3.qq.com') !== -1 ||
                logMsg.indexOf('btrace.video.qq.com')
              ) {
                return false;
              }
            } else if(logLevel === '4') {
              // 无效js错误过滤
              if(
                logMsg.indexOf('Script error') === 0 ||
                logMsg.indexOf('tipsToShow is not defined') !== -1 ||
                logMsg.indexOf('“txv”未定义') !== -1 ||
                logMsg.indexOf('chrome-extension') !== -1
              ) {
                return false;
              }
            }
          }
          return msg;
        }
    })
    window.aegis = aegis;
})()
</script>
